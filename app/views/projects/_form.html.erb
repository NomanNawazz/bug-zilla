  <br>
  <%= form_with model: project, local: true do |form| %>
    <%= form.hidden_field :manager_id, :value => current_user.id %>
    <div>
     <%= render :partial => "layouts/form_errors", :locals => { :model => project } %>
    </div>
    <div>
      <%= form.text_field :title, placeholder: :title, class: "form-control" %>
    </div>
    <br>
    <div>
      <%= form.text_field :description, placeholder: :description, class: "form-control" %>
    </div>
    <br>
    <div>
      <%= form.submit class: "btn btn-success" %>
    </div>
  <% end %>
  <%= link_to "Back", projects_path, class: "btn btn-secondary" %>

